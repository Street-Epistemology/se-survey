{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "firestore": {
      "rules": "firestore.rules.json",
      "indexes": "firestore.indexes.json"
    },
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
